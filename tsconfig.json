{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "esModuleInterop": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@environment": [ "src/environments/environment"],
      "@notifications": [ "src/app/modules/core/notifications/index"],
      "@notifications/testing": [ "src/app/modules/core/notifications/index.spec"],
      "@logging": [ "src/app/modules/core/logging/index" ],
      "@logging/testing": [ "src/app/modules/core/logging/index.spec" ],
      "@core": ["src/app/modules/core/core/index"],
      "@core/models/player": ["src/app/modules/core/core/models/player/index"],
      "@core/store/player": ["src/app/modules/core/core/store/player/index"],
      "@core/models/favorite-stations": ["src/app/modules/core/core/models/favorite-stations/index"],
      "@core/store/favorite-stations": ["src/app/modules/core/core/store/favorite-stations/index"],
      "@core/store/sleep-timer": ["src/app/modules/core/core/store/sleep-timer/index"],
      "@core/testing": ["src/app/modules/core/core/index.spec"],
      "@authentication": ["src/app/modules/core/authentication/index"],
      "@authentication/testing": ["src/app/modules/core/authentication/index.spec"],
      "@config": ["src/app/modules/core/config/index"],
      "@config/testing": ["src/app/modules/core/config/index.spec"],
      "@error-handling": ["src/app/modules/core/error-handling/index"],
      "@error-handling/testing": ["src/app/modules/core/error-handling/index.spec"],
      "@shared": ["src/app/modules/shared/shared/index"],
      "@root-components": ["src/app/modules/shared/root-components/index"],
      "@utilities": ["src/app/utilities/index"],
      "@utilities/testing": ["src/app/utilities/index.spec"]
    }
  }
}
