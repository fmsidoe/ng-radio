{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@notifications": [ "src/app/modules/core/notifications/index"],
      "@notifications/testing": [ "src/app/modules/core/notifications/index.spec"],
      "@logging": [ "src/app/modules/core/logging/index" ],
      "@logging/testing": [ "src/app/modules/core/logging/index.spec" ],
      "@core": ["src/app/modules/core/core/index"],
      "@core/testing": ["src/app/modules/core/core/index.spec"],
      "@keep-awake": ["src/app/modules/core/keep-awake/index"],
      "@keep-awake/testing": ["src/app/modules/core/keep-awake/index.spec"],
      "@authentication": ["src/app/modules/core/authentication/index"],
      "@authentication/testing": ["src/app/modules/core/authentication/index.spec"],
      "@config": ["src/app/modules/core/config/index"],
      "@config/testing": ["src/app/modules/core/config/index.spec"],
      "@root-state": ["src/app/modules/core/root-state/index"],
      "@root-state/favorite-stations": ["src/app/modules/core/root-state/sections/favorite-stations/index"],
      "@root-state/player": ["src/app/modules/core/root-state/sections/player/index"],
      "@error-handling": ["src/app/modules/core/error-handling/index"],
      "@error-handling/testing": ["src/app/modules/core/error-handling/index.spec"],
      "@shared-components": ["src/app/modules/shared/shared-components/index"],
      "@responsive-sidenav": ["src/app/modules/core/responsive-sidenav/index"],
      "@utilities": ["src/app/utilities/index"],
      "@utilities/testing": ["src/app/utilities/index.spec"]
    }
  }
}
