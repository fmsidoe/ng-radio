{
  "name": "ng-radio",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lint-fix": "tslint -c tslint.json \"src/**/*.ts\" --fix --project .",
    "test-single-headless": "ng test --watch=false --code-coverage --browsers=ChromeHeadless --reporters=progress,junit",
    "test-chrome": "ng test --browsers=Chrome",
    "test-firefox": "ng test --browsers=Firefox",
    "test:coveralls": "node ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info",
    "test:generate-publish-coverage": "npm run test-single-headless && npm run test:coveralls",
    "analyze-bundle": "ng build --prod --stats-json && webpack-bundle-analyzer dist/ng-radio/stats-es2015.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.5",
    "@angular/cdk": "~9.1.1",
    "@angular/common": "~9.0.5",
    "@angular/compiler": "~9.0.5",
    "@angular/core": "~9.0.5",
    "@angular/forms": "~9.0.5",
    "@angular/material": "^9.1.1",
    "@angular/platform-browser": "~9.0.5",
    "@angular/platform-browser-dynamic": "~9.0.5",
    "@angular/router": "~9.0.5",
    "@browninglogic/ng-loading-indicator": "3.0.1",
    "@browninglogic/ng-modal": "4.0.4",
    "@microsoft/applicationinsights-web": "^2.5.3",
    "@ngrx/effects": "^9.0.0",
    "@ngrx/router-store": "^9.0.0",
    "@ngrx/store": "^9.0.0",
    "@ngrx/store-devtools": "^9.0.0",
    "@types/lodash": "^4.14.149",
    "angular-oauth2-oidc": "^9.0.1",
    "core-js": "^3.6.4",
    "dayjs": "1.8.23",
    "is-blank": "^2.1.0",
    "jasmine-theories": "0.1.3",
    "lodash": "^4.17.15",
    "material-design-icons": "^3.0.1",
    "nosleep.js": "^0.9.0",
    "primeicons": "^2.0.0",
    "primeng": "^9.0.0",
    "rxjs": "~6.5.4",
    "subsink": "^1.0.1",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.5",
    "@angular/cli": "^9.0.5",
    "@angular/compiler-cli": "^9.0.5",
    "@angular/language-service": "~9.0.5",
    "@ngrx/schematics": "^9.0.0",
    "@types/jasmine": "~3.5.8",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^13.9.0",
    "codelyzer": "^5.2.1",
    "coveralls": "^3.0.9",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.2",
    "karma-junit-reporter": "^2.0.1",
    "protractor": "~5.4.3",
    "puppeteer": "^2.1.1",
    "ts-node": "~8.6.2",
    "tslint": "^6.1.1",
    "tslint-etc": "^1.10.1",
    "typescript": "3.7.5",
    "webpack-bundle-analyzer": "^3.6.1"
  }
}
