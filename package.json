{
  "name": "ng-radio",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lint-fix": "tslint -c tslint.json \"src/**/*.ts\" --fix --project .",
    "test-single-headless": "ng test --watch=false --code-coverage --browsers=ChromeHeadless --reporters=progress,junit",
    "test-chrome": "ng test --browsers=Chrome",
    "test-firefox": "ng test --browsers=Firefox",
    "test:coveralls": "node ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info",
    "test:generate-publish-coverage": "npm run test-single-headless && npm run test:coveralls",
    "analyze-bundle": "ng build --prod --stats-json && webpack-bundle-analyzer dist/ng-radio/stats-es2015.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.5",
    "@angular/cdk": "~9.2.1",
    "@angular/common": "~10.0.5",
    "@angular/compiler": "~10.0.5",
    "@angular/core": "~10.0.5",
    "@angular/forms": "~10.0.5",
    "@angular/material": "^9.2.1",
    "@angular/platform-browser": "~10.0.5",
    "@angular/platform-browser-dynamic": "~10.0.5",
    "@angular/router": "~10.0.5",
    "@microsoft/applicationinsights-web": "^2.5.4",
    "@ngrx/effects": "^9.1.0",
    "@ngrx/router-store": "^9.1.0",
    "@ngrx/store": "^9.1.0",
    "@ngrx/store-devtools": "^9.1.0",
    "angular-oauth2-oidc": "^9.2.1",
    "core-js": "^3.6.5",
    "dayjs": "1.8.25",
    "is-blank": "^2.1.0",
    "jasmine-theories": "0.1.3",
    "lodash": "^4.17.19",
    "mat-progress-buttons": "^9.1.1",
    "material-design-icons": "^3.0.1",
    "ngx-mat-select-search": "^2.1.2",
    "nosleep.js": "^0.9.0",
    "primeicons": "^2.0.0",
    "primeng": "^9.0.6",
    "rxjs": "~6.5.5",
    "subsink": "^1.0.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.4",
    "@angular/cli": "^10.0.4",
    "@angular/compiler-cli": "^10.0.5",
    "@angular/language-service": "~10.0.5",
    "@ngrx/schematics": "^9.1.0",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.8",
    "@types/lodash": "^4.14.150",
    "@types/node": "^13.13.4",
    "codelyzer": "^6.0.0",
    "coveralls": "^3.1.0",
    "jasmine-core": "~3.5.0",
    "jasmine-marbles": "^0.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-junit-reporter": "^2.0.1",
    "protractor": "~7.0.0",
    "puppeteer": "2.1.1",
    "ts-node": "~8.9.1",
    "tslint": "~6.1.0",
    "tslint-etc": "^1.10.1",
    "typescript": "3.9.7",
    "webpack-bundle-analyzer": "^3.7.0"
  }
}
